
proc putchar(c 'int) 'int;

proc exit(status 'int) {
    asm("
    mov rax, 60
    mov rdi, {}
    syscall
    ", status);
}

proc putc(c 'int) {
    let buf 'int = c;

    asm("
    mov rax, 1
    mov rdi, 1
    lea rsi, {}
    mov rdx, 1
    syscall
    ", buf);
}

proc add(a 'int, b 'int) 'int {
    return a + b;
}

proc _start() {

    let x 'int = 123;

    putc(add(64, 1));

    exit(0);
}
