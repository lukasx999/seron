proc malloc(size int) *void;
proc free(p *void);
proc puts(s *char);

proc main() int {

    let xs *char = malloc(50);
    *(xs+0L)  = 'H';
    *(xs+1L)  = 'e';
    *(xs+2L)  = 'l';
    *(xs+3L)  = 'l';
    *(xs+4L)  = 'o';
    *(xs+5L)  = ',';
    *(xs+6L)  = ' ';
    *(xs+7L)  = 'W';
    *(xs+8L)  = 'o';
    *(xs+9L)  = 'r';
    *(xs+10L) = 'l';
    *(xs+11L) = 'd';
    *(xs+12L) = '!';
    *(xs+13L) = 0;



    puts(xs);

    free(xs);

    return 0;
}
