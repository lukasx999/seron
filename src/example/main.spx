proc exit() {
    asm("mov rax, 60");
    asm("mov rdi, 45");
    asm("syscall");
}

proc print() {
    # val buf 'int = 70-(5+1);
    val buf 'int = (65*100)/100+1;
    asm("mov rax, 1");
    asm("mov rdi, 1");
    asm("lea rsi, {}", buf);
    asm("mov rdx, 1");
    asm("syscall");

}

proc _start() {

    if 1 {
        print();
    }

    if 1 {
        print();
    }

    while 0 {
        print();
    }

    while 1 {
        print();
    }


    exit();
}
